	<queryString><![CDATA[SELECT
	FORMULARIOCONFIG.NAME_FORMULARIO AS NOME,
	ITENS.TX_FIELD_FORMULARIO AS FORMULARIO
FROM TASK TAREFAS
INNER JOIN DESCRIPTIONFORMULARIOTASK ITENS
	ON ITENS.PK_SEQUENCE_TASK = TAREFAS.PK_NUMBER_SEQUENCE_TASK
	AND ITENS.PK_YEAR_START_TASK = TAREFAS.PK_YEAR_START_TASK
INNER JOIN FORMULARIOCONFIGURATIONTASK FORMULARIOCONFIG
	ON FORMULARIOCONFIG.PK_CONFIG_TASK = ITENS.PK_CONFIG_TASK AND
	   FORMULARIOCONFIG.PK_ORDER_FORMULARIO = ITENS.PK_ORDER_FORMULARIO AND
	   FORMULARIOCONFIG.FLAG_FORMULARIO_VALIDITY='S'
WHERE TAREFAS.PK_YEAR_START_TASK = $P{ano} AND
      TAREFAS.PK_NUMBER_SEQUENCE_TASK = $P{numero} 
ORDER BY ITENS.PK_ORDER_FORMULARIO
]]></queryString>

