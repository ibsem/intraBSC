
package br.com.intraPRO.modelo;

import java.sql.Timestamp;

import br.com.intraBSC.modelo.UsuarioTO;

/**
 * Classe que representa os campos que devem ser transportados entre as camadas.
 */
public class AnotacaoTarefaTO {

	private String textoAnotacao;
	private String valoresDatasEtapas;	//campo referente ou sim ou não do form
	private int anoCriacao;
	private int codTarefa;
	private Timestamp tsAnotacao;
	private int codTipoAnotacao;
	private int numeroOrdemEtapa;
	private int codUsuario;
	private int codConfigTarefa;
	private String etapa;
	private String dataAnotacao;
	private UsuarioTO responsavel;

	
	public String getTextoAnotacao() {
		return textoAnotacao;
	}

	public void setTextoAnotacao(String textoAnotacao) {
		this.textoAnotacao = textoAnotacao;
	}

	public String getValoresDatasEtapas() {
		return valoresDatasEtapas;
	}

	public void setValoresDatasEtapas(String valoresDatasEtapas) {
		this.valoresDatasEtapas = valoresDatasEtapas;
	}

	public AnotacaoTarefaTO() {
	}
	public int getAnoCriacao() {
		return anoCriacao;
	}

	public void setAnoCriacao(int anoCriacao) {
		this.anoCriacao = anoCriacao;
	}

	public int getCodTarefa() {
		return codTarefa;
	}

	public void setCodTarefa(int codTarefa) {
		this.codTarefa = codTarefa;
	}

	public int getCodConfigTarefa() {
		return codConfigTarefa;
	}

	public void setCodConfigTarefa(int codConfigTarefa) {
		this.codConfigTarefa = codConfigTarefa;
	}

	public int getCodTipoAnotacao() {
		return codTipoAnotacao;
	}

	public void setCodTipoAnotacao(int codTipoAnotacao) {
		this.codTipoAnotacao = codTipoAnotacao;
	}

	public int getCodUsuario() {
		return codUsuario;
	}

	public void setCodUsuario(int codUsuario) {
		this.codUsuario = codUsuario;
	}

	public int getNumeroOrdemEtapa() {
		return numeroOrdemEtapa;
	}

	public void setNumeroOrdemEtapa(int numeroOrdemEtapa) {
		this.numeroOrdemEtapa = numeroOrdemEtapa;
	}

	public Timestamp getTsAnotacao() {
		return tsAnotacao;
	}

	public void setTsAnotacao(Timestamp tsAnotacao) {
		this.tsAnotacao = tsAnotacao;
	}
	
	public String getEtapa() {
		return etapa;
	}

	public void setEtapa(String etapa) {
		this.etapa = etapa;
	}
	
	/**
	 * @return Returns the dataAnotacao.
	 */
	public String getDataAnotacao() {
		return dataAnotacao;
	}

	
	/**
	 * @param dataAnotacao The dataAnotacao to set.
	 */
	public void setDataAnotacao(String dataAnotacao) {
		this.dataAnotacao = dataAnotacao;
	}

	public UsuarioTO getResponsavel() {
		return responsavel;
	}

	public void setResponsavel(UsuarioTO responsavel) {
		this.responsavel = responsavel;
	}
}
