package br.com.intraPRO.excecoes;


public class ExceptionNegocioPRO extends ExceptionPRO {
	public ExceptionNegocioPRO() {
	}
	
	public ExceptionNegocioPRO(Throwable t){
		super(t);
	}
	
	public ExceptionNegocioPRO(String key) {
		super(key);
	}
	
	public ExceptionNegocioPRO(String key, Object values[]) {
        super(key,values);
	}
	
	public ExceptionNegocioPRO(String key, Object value0) {
        this(key, new Object[] {
            value0
        });
    }

    public ExceptionNegocioPRO(String key, Object value0, Object value1) {
        this(key, new Object[] {
            value0, value1
        });
    }

    public ExceptionNegocioPRO(String key, Object value0, Object value1, Object value2) {
        this(key, new Object[] {
            value0, value1, value2
        });
    }

    public ExceptionNegocioPRO(String key, Object value0, Object value1, Object value2, Object value3) {
        this(key, new Object[] {
            value0, value1, value2, value3
        });
    }
}
